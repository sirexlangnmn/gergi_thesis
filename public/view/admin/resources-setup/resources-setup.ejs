<style>
    .widget-post-bx {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); /* Two columns, responsive */
        gap: 20px; /* Spacing between items */
    }

    .widget-post {
        display: flex;
        align-items: center;
        background: #f1f5f9;
        padding: 10px;
        border-radius: 10px;
    }

    .dz-media {
        flex: 0 0 100px; /* Fixed width for images */
        margin-right: 15px;
    }

    .dz-media img {
        width: 100px; /* Make sure images fit */
        height: auto;
        border-radius: 5px;
    }

    .dz-info {
        flex: 1;
    }

    .title {
        font-size: 16px;
        font-weight: bold;
    }

    .dz-meta {
        font-size: 14px;
        color: gray;
    }

    /* Responsive: Stack items in one column on smaller screens */
    @media (max-width: 600px) {
        .widget-post-bx {
            grid-template-columns: 1fr; /* Single column on small screens */
        }

        .widget-post {
            flex-direction: column;
            text-align: center;
        }

        .dz-media {
            margin-right: 0;
            margin-bottom: 10px;
        }
    }
</style>

